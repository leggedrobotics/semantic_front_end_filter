<launch>
  <!-- Elevation mapping node -->
  <param name="/use_sim_time" value="true"/>
  <node pkg="elevation_mapping_cupy" type="elevation_mapping_node" name="elevation_mapping_raw" output="screen">
    <rosparam command="load" file="$(find semantic_front_end_filter_ros)/configs/elevmap_raw.yaml" />
  </node>
  <node pkg="elevation_mapping_cupy" type="elevation_mapping_node" name="elevation_mapping_pred" output="screen">
    <rosparam command="load" file="$(find semantic_front_end_filter_ros)/configs/elevmap_pred.yaml" />
  </node>
</launch>
